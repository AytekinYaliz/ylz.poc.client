{
   "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:react/recommended"
   ],
   "plugins": [
      "flowtype"
   ],
   "parser": "babel-eslint",
   "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
         "jsx": true,
         "experimentalObjectRestSpread": true
      }
   },
   "env": {
      "es6": true,
      "browser": true,
      "node": true
   },
   "rules": {
      "quotes": 0,
      "no-console": 2,
      "no-debugger": 2,
      "no-var": 2,
      "semi": [
         2,
         "always"
      ],
      "no-unused-vars": 1,
      "no-trailing-spaces": 0,
      "eol-last": 0,
      "import/named": 1,
      "no-underscore-dangle": 2,
      "no-alert": 2,
      "no-lone-blocks": 2,
      "react/no-did-mount-set-state": 2,
      "react/no-did-update-set-state": 2,
      "react/no-direct-mutation-state": 2,
      "react/no-multi-comp": 0,
      "react/no-set-state": 0
   },
   "globals": {
      "require": true,
      "module": true,
      "global": true,
      "window": true,
      "describe": true,
      "it": true,
      "beforeAll": true,
      "beforeEach": true,
      "afterEach": true,
      "afterAll": true,
      "expect": true,
      "__dirname": true,
      "spyOn": true
   }
}